{
  "name": "mongoose-tsgen",
  "description": "A Typescript interface generator for Mongoose that works out of the box.",
  "version": "0.0.4",
  "author": "Francesco Virga @francescov1",
  "bin": {
    "mtgen": "./bin/run"
  },
  "bugs": "https://github.com/Bounced-Inc/mongoose-tsgen/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "flat": "^5.0.2",
    "mongoose": "^5.10.6",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/flat": "^5.0.1",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^10.17.35",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Bounced-Inc/mongoose-tsgen",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mtgen",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Bounced-Inc/mongoose-tsgen",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "test": "echo NO TESTS",
    "readme": "./node_modules/@oclif/dev-cli/bin/run readme"
  },
  "types": "lib/index.d.ts"
}
